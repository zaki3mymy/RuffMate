      "fixedExample": "import os\nimport sys",
      "legendInfo": {
        "status": "stable",
        "fixable": true
      }
    },
    {
      "code": "Q000",
      "name": "bad-quotes-inline-string",
      "category": "flake8-quotes",
      "description": "Single quotes found but double quotes preferred",
      "example": "x = 'hello'",
      "fixedExample": "x = \"hello\"",
      "legendInfo": {
